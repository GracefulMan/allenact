vs3

sudo python rl_robothor/docker/startx.py &> ~/logxserver &
ofolder=~/work/data/the-robot-project/camera-ready/
tensorboard --bind_all --logdir $ofolder &> ~/logtensorboard &
mkdir -p $ofolder
rm -r $ofolder/*
python main.py -o $ofolder -s 12345 objectnav_robothor_camera_ready_old_cvpr_model_image_detector_noshaping &> $ofolder/log_exp &
tail -f $ofolder/log_exp


